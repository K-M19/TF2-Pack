"VertexLitGeneric"
{
	"$basetexture"	"models/weapons/c_items/c_rainblower_blue"
	"$bumpmap" "models/weapons/c_items/c_rainblower_normal"
	"$normalmapalphaenvmapmask" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.085 .085 .085]"

	"$phong" "1"
	"$phongexponent" "36"
	"$phongboost" ".8"
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "1"
	
	"$rimlight" "1"
	"$rimlightexponent" "10"
	"$rimlightboost" "1"
	
	"$yellow" "0"
	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}